<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
		<link rel="stylesheet" href="{{asset('assets/css/bootstrap.min.css')}}" crossorigin="anonymous">
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
			<script src="{{asset('assets/jquery/jquery-3.2.1.min.js')}}" ></script>
			<script src="{{asset('assets/popper/popper.min.js')}}" crossorigin="anonymous"></script>
			<script src="{{asset('assets/js/bootstrap.min.js')}}" crossorigin="anonymous"></script>
		{% endblock %}
    </body>
</html>
